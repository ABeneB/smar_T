<%= simple_form_for @order do |f| %>
  <%= f.association :costumer %>
  <%= f.input :adress %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :duration_pickup %>
  <%= f.input :duration_delivery %>
  <%= f.input :capacity %>
  <%= f.input :comment %>
  <%= f.button :submit %>
<% end %>